# /etc/cryptmount/cmtab - encrypted filesystem information for cryptmount
# try 'man 8 cryptmount' or 'man 5 cmtab' for more details

home-ubuntu-crypt {
  dev=/home/ubuntu/local/crypt.fs
  dir=/home/ubuntu/crypt
  fstype=ext3
  fsoptions=noatime
  keyfile=/home/ubuntu/local/crypt.key
  flags=user,nofsck
}

